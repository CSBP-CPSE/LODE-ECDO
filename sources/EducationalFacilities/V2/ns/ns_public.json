{
  "localfile": "ns/directoryofpublicschools2019-2020_isced.csv",
  "schema_groups": ["edu", "address", "geocoordinates"],
  "source": "http://ns-schools.ednet.ns.ca/",
  "provider": "Province of Nova Scotia",
  "encoding": "cp1252",
  "format": {
      "type": "csv",
      "delimiter": ",",
      "quote": "\""
  },
  "schema": {
      "edu" : {
          "facility_name": "School Name",
          "source_id": "Sch Cd",
          "facility_type": "force:Public",
          "ISCED010": "ISCED010",
          "ISCED020": "ISCED020",
          "ISCED1": "ISCED1",
          "ISCED2": "ISCED2",
          "ISCED3": "ISCED3",
          "authority_name": "Region Name",
          "telephone": "Phone",
	  "grade_range": ["Start Gr", "force: - ", "End Gr"]
	  
      },

      "address" : {
          "address_str": ["Civic Address","City","Postal Code","force:NS"],
          "street_addr": "Civic Address",
          "postal_code": "Postal Code",
	  "city": "City"
          }
  }
}
