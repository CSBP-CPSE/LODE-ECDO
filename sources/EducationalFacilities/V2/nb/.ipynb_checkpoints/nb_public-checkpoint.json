{
    "localfile": "nb/NB_PublicSchools_geonb_isced.csv",
    "schema_groups": ["edu", "address", "geocoordinates"],
    "provider": "Province of New Brunswick",
    "encoding": "cp1252",
    "format": {
      "type": "csv",
      "delimiter": ",",
      "quote": "\""
  },
  "schema": {
      "edu" : {
          "facility_name": "strNM",
          "source_id": "strID",
          "ISCED020": "ISCED020",
         "ISCED1": "ISCED1",
         "ISCED2": "ISCED2",
         "ISCED3": "ISCED3",
          "facility_type": "force:Public",
          "grade_range": "strGR",
          "geo_source": "force:source",
          "authority_name": "strDST"
      },

      "address" : {
          "address_str": ["strAD1","strAD2","StrCM","strPC","force:NB"],
          "city": "StrCM",
          "province": "force:NB",
          "postal_code": "strPC"
          },
	"geocoordinates": {
	"latitude":"latitude",
	"longitude":"longitude"
	}
  }
}
