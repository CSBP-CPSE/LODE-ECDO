{
    "localfile": "on/on_private_schools_contact_information_october_2020_en_isced.csv",
    "schema_groups": ["edu", "address", "geocoordinates"],
    "source": "https://data.ontario.ca/dataset/private-school-contact-information",
    "licence": "https://www.ontario.ca/page/open-government-licence-ontario",
    "provider": "Province of Ontario",
    "encoding": "cp1252",
    "format": {
      "type": "csv",
      "delimiter": ",",
      "quote": "\""
  },
  "schema": {
      "edu" : {
          "facility_name": "School Name",
          "source_id": "School Number",
          "grade_type": "School Level",
          "ISCED010":"ISCED010",
          "ISCED020":"ISCED010",
         "ISCED1": "ISCED1",
         "ISCED2": "ISCED2",
         "ISCED3": "ISCED3",
          "facility_type":"force:Private"
      },

      "address" : {
          "street_addr": "Street Address",
          "address_str": ["Street Address", "City", "Province","Postal Code"],
          "city": "City",
          "province": "Province",
          "postal_code": "Postal Code",
          "telephone": "Telephone Number"
          }
  }
}
